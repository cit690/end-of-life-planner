  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
      <a href="/" <%- title === 'Home' ? 'class="active"' : "" %> >Home</a>
      <a href="/plans" <%- title === 'Your Death Plans' ? 'class="active"' : "" %> >All Plans</a>
      <a 
      href="/dispositions/new" <%- title === 'Add Disposition Method' ? 'class="active"' : "" %> >Add Disposition Method</a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
